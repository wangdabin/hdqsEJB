hdqs.query.handle.file.threshold.ctrl=0
hdqs.query.handle.file.size.threshold=20
#B KB MB
hdqs.query.handle.file.size.unit=MB
hdqs.query.handle.file.count.threshold=20

#打印文本文件每页显示行数
hdqs.query.print.txt.line.per.page=35
#导出及异步生成PDF文件每页显示行数
