ftp.upload.path=/data/ftp
ftp.bak.path=/data/ftp/bak
ftp.download.path=/data/ftp
##generate pdf path
hdqs.query.output.tftp.path=/data/ftp

cache.expired.minute.threshold=20
db.synchronize.hours.before.threshold=1
